    "skos:definition": [
        {
            "@language": "es",
            "@value": "un movimiento o disposición de los brazos o de las manos en forma codificada para guiar a las personas que estén realizando maniobras que constituyan un riesgo o peligro para los trabajadores"
        },
        {
            "@language": "en",
            "@value": "movement and/or position of the arms and/or hands in coded form for guiding persons who are carrying out manoeuvres which constitute a hazard or danger for workers"
        },
        {
            "@language": "de",
            "@value": "eine codierte Bewegung und/oder Stellung von Armen und/oder Händen zur Anleitung von Personen bei Handhabungsvorgängen die ein Risiko oder eine Gefahr für Arbeitnehmer darstellen"
        },
        {
            "@language": "nl",
            "@value": "een beweging en/of stand van de armen en/of handen in gecodeerde vorm bedoeld om aanwijzingen te geven aan personen die handelingen verrichten waarbij risico of gevaar voor werknemers bestaat"
        },
        {
            "@language": "en",
            "@value": "a vote where each person who is entitled to vote has just one votehowever many shares he may have"
        },
        {
            "@language": "de",
            "@value": "unmaßstäbliche skizzenmäßige Darstellung die bei Aufmessungen Beobachtungen und Untersuchungen hergestellt wird"
        }
