    "skos:definition": [
        {
            "@language": "en",
            "@value": "a clause in a legal instrument providing for the assessment of a penalty under certain stated conditions"
        },
        {
            "@language": "de",
            "@value": "in einem Vertrag fuer den Fall der Nichterfuellung einer Verbindlichkeit vorgesehene Geldstrafe"
        },
        {
            "@language": "de",
            "@value": "eine Vertragsstrafe (Konventionalstrafe) liegt vor wenn der Schuldner dem Gläubiger für den Fall daß er seine Verbindlichkeit nicht oder nicht in gehöriger Weise erfüllt die Zahlung einer Geldsumme als Strafe verspricht"
        },
        {
            "@language": "de",
            "@value": "wird zwischen zwei Vertragsparteien für den Fall vereinbart daß eine Partei die geschuldete Leistung nicht wie vereinbart erbringt"
        },
        {
            "@language": "en",
            "@value": "penalty clause stipulated in a contract to estimate damages in the event of non-performance or breach of contract"
        }
    ]
