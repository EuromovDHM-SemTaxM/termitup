    "skos:definition": [
        {
            "@language": "es",
            "@value": "En sentido amplio la legítima defensa ha de ser entendida como la necesaria para impedir o repeler una agresión injusta contra la persona los derechos o los bienes propios o ajenos."
        },
        {
            "@language": "de",
            "@value": "gerechtfertigte Abwehr eines rechtswidrigen (gewaltsamen) Angriffs"
        },
        {
            "@language": "nl",
            "@value": "Niet strafbaar is hij die een feit begaat geboden door de noodzakelijke verdediging van eigen of eens anders lijf eerbaarheid of goed tegen ogenblikkelijke wederrechtelijke aanranding."
        }
