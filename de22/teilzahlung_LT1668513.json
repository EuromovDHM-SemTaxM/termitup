    "skos:definition": [
        {
            "@language": "en",
            "@value": "Intermediate payment made during the course of a contract or transaction on evidence of progress being made"
        },
        {
            "@language": "de",
            "@value": "anders als die bloße Anzahlung ist die Abschlagszahlung regelmäßig vorweggenommene (Teil-)Erfüllung (z. B. Zahlung auf bereits geleistete aber noch nicht abgerechnete Arbeit) nicht Vorschuss (auf künftig fällig werdende Leistungen) oder Darlehensvertrag"
        },
        {
            "@language": "en",
            "@value": "proportional payment of all debts in the event of the debtor's resources being insufficient to meet all the sums outstanding in full"
        },
        {
            "@language": "de",
            "@value": "gleichmaessige Teilzahlung einer Schuld durch den Schuldner an jeden seiner Glaeubiger wenn ihm seine Mittel keine vollstaendige Abloesung der faelligen Foerderungen erlauben"
        }
    ]
