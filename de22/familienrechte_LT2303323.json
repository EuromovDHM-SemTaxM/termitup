    "skos:definition": [
        {
            "@language": "en",
            "@value": "branch of law dealing with matters relating to the family such as inheritance marriage divorce or custody of children"
        },
        {
            "@language": "de",
            "@value": "Teilgebiet des Zivilrechts das die Rechtsverhältnisse der durch Ehe Lebenspartnerschaft Familie und Verwandtschaft miteinander verbundenen Personen und die außerhalb der Verwandtschaft bestehenden gesetzlichen Vertretungsfunktionen wie Vormundschaft Pflegschaft und rechtliche Betreuung regelt"
        },
        {
            "@language": "nl",
            "@value": "recht dat de rechtsbetrekkingen regelt tussen personen die verbonden zijn door afstamming (verwantschap tussen bijvoorbeeld een kind en zijn moeder of vader) of door huwelijk (of geregistreerd partnerschap)"
        },
        {
            "@language": "de",
            "@value": "Gesamtheit der staatlichen Rechtsnormen welche die rechtlichen Beziehungen der Mitglieder der Familie zueinander und zu Dritten regeln"
        }
    ]
