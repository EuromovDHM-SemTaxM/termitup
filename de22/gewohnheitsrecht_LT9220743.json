    "skos:definition": [
        {
            "@language": "es",
            "@value": "Reglas basadas en la costumbre esto es en normas habitualmente no expresadas por escrito que resultan de prácticas reiteradas generalmente asumidas por la mayoría de los que están en un lugar o participan en una determinada situación."
        },
        {
            "@language": "en",
            "@value": "traditional common rule or practice that has become an intrinsic part of the accepted and expected conduct in a community profession or trade and is treated as a legal requirement"
        },
        {
            "@language": "de",
            "@value": "Gesamtheit der ungeschriebenen Rechtsnormen die sich durch ständige Übung gebildet haben und auf dem allgemeinen Rechtsbewusstsein beruhen"
        },
        {
            "@language": "nl",
            "@value": "Recht dat blijkens een bestaand gebruik in de samenleving als zodanig wordt geaccepteerd"
        },
        {
            "@language": "de",
            "@value": "Gesamtheit der Rechtsvorschriften die sich in langjaehriger Uebung herausgebildet haben"
        }
    ],
