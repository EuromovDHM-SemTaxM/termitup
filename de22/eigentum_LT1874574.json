    "skos:definition": [
        {
            "@language": "es",
            "@value": "Derecho de gozar y disponer de una cosa sin más limitaciones que las establecidas en las leyes."
        },
        {
            "@language": "en",
            "@value": "The exclusive right of possessing enjoying and disposing of a thing."
        },
