    "skos:definition": [
        {
            "@language": "en",
            "@value": "In relation to an offender aged 21 years or over a sentence of imprisonment and under that age detention in a young offender institution. [UK_EAW] [custodial sentence]"
        },
        {
            "@language": "de",
            "@value": "Im Entzug der körperlichen Bewegungsfreiheit bestehende Strafe die in einer Strafvollzugsanstalt vollstreckt wird. [DE]"
        },
        {
            "@language": "nl",
            "@value": "Straf die de vrijheid van de persoon aantast: gevangenisstraf hechtenis [NL] // Hoofdstraf bestaande in het ontnemen aan de veroordeelde van zijn grondrecht om vrij te komen en gaan. [BE]"
        },
