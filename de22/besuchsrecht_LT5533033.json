    "skos:definition": [
        {
            "@language": "en",
            "@value": "in family law the right granted by a court to a parent or other relative who is deprived of custody1 of a child to visit the child on a regular basis"
        },
        {
            "@language": "de",
            "@value": "im Familienrecht unabhängig von der Regelung der elterlichen Sorge bestehendes Recht zum persönlichen Umgang mit dem Kind und des Kindes mit seinen Bezugspersonen"
        },
        {
            "@language": "de",
            "@value": "Recht des Elternteils der mit dem minderjährigen Kind nicht im gemeinsamen Haushalt lebt und des Kindes miteinander persönlich zu verkehren."
        },
        {
            "@language": "nl",
            "@value": "Recht v.d. niet met de voogdij belaste ouder."
        }
    ]
