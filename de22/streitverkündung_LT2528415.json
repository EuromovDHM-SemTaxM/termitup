    "skos:definition": [
        {
            "@language": "en",
            "@value": "Notice served on a third party in order to initiate proceedings against a person not already a party to the civil action under way"
        },
        {
            "@language": "de",
            "@value": "die im zivil- und arbeitsrechtlichen Verfahren - mit Ausnahme des Beschlussverfahrens - mögliche förmliche Benachrichtigung e. Dritten von einem anhängigen Rechtstreit durch eine Partei"
        },
        {
            "@language": "nl",
            "@value": "beroep door de gewaarborgde op een ander aan wie men de beweerde aansprakelijkheid ontleent teneinde het verhaal op deze te laten zoeken"
        },
        {
            "@language": "de",
            "@value": "Benachrichtigung eines Dritten durch eine Partei in einem Zivilprozeß gegen den diese im Fall ihres Unterliegens einen Regreßanspruch zu haben glaubt mit der Aufforderung sie im Prozeß zu unterstützen"
        }
    ]
