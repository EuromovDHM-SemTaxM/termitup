    "skos:definition": [
        {
            "@language": "en",
            "@value": "a situation that exists when it is agreed that the purchaser must pay the whole or part of the purchase price before the goods are delivered"
        },
        {
            "@language": "de",
            "@value": "bei einem Kaufvertrag bei dem der Kaeufer zur Zahlung vor Lieferung des Kaufgegenstandes verpflichtet ist"
        },
        {
            "@language": "es",
            "@value": "el pago de un importe igual a la restitución a la exportación antes de dicha exportación cuando tal pago esté previsto en el Reglamento (CEE) no 565/80"
        },
        {
            "@language": "en",
            "@value": "the payment of an amount equal to the export refund before the goods are exported where such payment is provided for in Regulation (EEC) No 565/80"
        },
        {
            "@language": "de",
            "@value": "die Zahlung eines der Ausfuhrerstattung entsprechenden Betrages vor der Ausfuhr der Waren sofern dies in der Verordnung (EWG) Nr.565/80 vorgesehen ist"
        },
        {
            "@language": "nl",
            "@value": "de betaling voorafgaand aan de uitvoer van een bedrag dat gelijk is aan de uitvoerrestitutie wanneer Verordening (EEG) nr.565/80 in een dergelijke betaling voorziet"
        }
