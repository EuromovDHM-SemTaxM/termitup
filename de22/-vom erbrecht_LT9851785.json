    "skos:definition": [
        {
            "@language": "es",
            "@value": "Parte del Derecho Civil que estudia en lo teórico y regula en lo práctico lo atinente a las transmisiones patrimoniales y de otros derechos por causa de muerte IATE:872358 ."
        },
        {
            "@language": "en",
            "@value": "branch of law regulating the devolution of property on an heir or heirs upon the death of its owner"
        },
        {
            "@language": "de",
            "@value": "Gesamtheit aller privatrechtlichen Vorschriften die den Übergang der Erbschaft vom Erblasser auf dessen Erben regeln"
        },
        {
            "@language": "nl",
            "@value": "\"het geheel van rechtsregels betreffende de overgang van het vermogen (boedel nalatenschap) van een overledene (erflater) op een of meer andere nog levende personen (erfgenamen legatarissen)\""
        },
        {
            "@language": "en",
            "@value": "statutory rules of inheritance of a dead person's estate when the property is not given by the terms of a will"
        },
        {
            "@language": "nl",
            "@value": "\"deel van het burgerlijk recht dat de erfopvolging of successie regelt dat wil zeggen regelt wat er gebeurt met bezittingen en schulden van iemand als die overlijdt\""
        },
        {
            "@language": "nl",
            "@value": "De erfgenamen van een overledene zijn onder meer zijn afstammelingen: zijn (al dan niet binnen het huwelijk geboren) kinderen kleinkinderen etc. [BE]"
        }
    ]
