    "skos:definition": [
        {
            "@language": "es",
            "@value": "Hay comunidad [de bienes] cuando la propiedad de una cosa o de un derecho pertenece pro indiviso a varias personas."
        },
        {
            "@language": "en",
            "@value": "situation in which a person or organisation shares ownership in an asset with another individual or group"
        },
        {
            "@language": "de",
            "@value": "Eigentum das mehr als einer Partei gehört die alle gleiche Rechte an dem Besitz haben"
        },
        {
            "@language": "nl",
            "@value": "meerdere personen zijn tegelijk eigenaar van hetzelfde goed: het eigendomsrecht is verdeeld maar het goed niet - dat behoort aan ieder van hen voor een wiskundig breukdeel"
        }
