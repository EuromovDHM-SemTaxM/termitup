    "skos:definition": [
        {
            "@language": "es",
            "@value": "Toda persona física o jurídica responsable de los animales tanto de forma permanente como temporal inclusive durante su transporte o en un mercado."
        },
        {
            "@language": "en",
            "@value": "natural or legal person responsible for animals whether on a permanent or temporary basis including during transportation or at a market"
        },
        {
            "@language": "de",
            "@value": "jede natürliche oder juristische Person die ständig oder vorübergehend auch beim Tiertransport oder auf dem Viehmarkt für Tiere verantwortlich ist"
        },
        {
            "@language": "nl",
            "@value": "natuurlijke persoon of rechtspersoon die permanent of tijdelijk verantwoordelijk is voor dieren"
        },
        {
            "@language": "en",
            "@value": "a natural or legal person responsible for animals"
        },
        {
            "@language": "de",
            "@value": "jede natürliche oder juristische Person ausgenommen Transportunternehmer die dauerhaft oder zeitweilig für Tiere zuständig ist oder mit ihnen umgeht"
