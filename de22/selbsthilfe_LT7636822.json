    "skos:definition": [
        {
            "@language": "nl",
            "@value": "Vertrouwen op eigen kracht. Geldt voornamelijk voor groepen."
        },
        {
            "@language": "de",
            "@value": "unter Selbsthilfe ist die aktive Teilnahme von Bevoelkerungsgruppen an der wirtschaftliche und sozialen Entwicklung jeweiliger EL zu verstehen"
        },
        {
            "@language": "nl",
            "@value": "Omvat het geheel van aktiviteiten van groepen mensen die in eenzelfde probleemsituatie verkeren: AA-groepen Fobieclub Vrouwen in de overgang etc (MBZ: Wegwijzer Maatschappelijk Werk)"
        },
        {
            "@language": "de",
            "@value": "Sozialarbeit mit dem Ziel Aktivierung des Klienten zur Selbsthilfe ist effektiver und dadurch kostensparend auf Zeit."
        }
