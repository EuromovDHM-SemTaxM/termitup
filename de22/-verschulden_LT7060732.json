    "skos:definition": [
        {
            "@language": "en",
            "@value": "an act of misconduct through which the employee is deemed to have broken his contract of employment and which constitutes just cause for summary dismissali.e.without notice"
        },
        {
            "@language": "de",
            "@value": "mitwirkendes Verschulden (des Schädigers und des Geschädigten) (Doucet-Fleck Wörterbuch der Rechts- und Wirtschaftssprache 5. Aufl.)"
        },
        {
            "@language": "en",
            "@value": "a fundamental breach of contract by the employee such that the necessary bond of mutual trust and confidence between employer and employee is broken and the possibility of continued employment must be in grave doubt"
        },
        {
            "@language": "de",
            "@value": "vorsätzliches oder fahrlässiges vertragswidriges Verhalten eines Arbeitnehmers"
        }
