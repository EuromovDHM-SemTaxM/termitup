    "skos:definition": [
        {
            "@language": "en",
            "@value": "programme aimed at helping vulnerable parents with additional social and/or healthcare needs to improve their relationship with their child based on principles of attachment theory"
        },
        {
            "@language": "de",
            "@value": "Begleitprogramm für Eltern mit besonderem gesundheitlichen oder sozialen Förderbedarf das auf Grundlage der Bindungstheorie zur Förderung der frühkindlichen Entwicklung und der Eltern-Kind-Beziehung beitragen soll"
        },
        {
            "@language": "nl",
            "@value": "verzamelbegrip voor preventieve activiteiten en interventies die tot doel hebben de opvoedingscompetenties van ouders te vergroten en de opvoedingssituatie te verbeteren"
        }
