    "skos:definition": [
        {
            "@language": "en",
            "@value": "a clause in a legal instrument providing for the assessment of a penalty under certain stated conditions"
        },
        {
            "@language": "de",
            "@value": "in einem Vertrag fuer den Fall der Nichterfuellung einer Verbindlichkeit vorgesehene Geldstrafe"
        },
        {
            "@language": "es",
            "@value": "\"Cláusula penal en un contrato o concesión que obliga al pago de una cantidad proporcionada al retraso en el cumplimiento\"(N. Amorós O. Merlin; Dicc. Jurídico Ed. Navarre 1986)&lt;16 dr"
        },
        {
            "@language": "de",
            "@value": "eine Vertragsstrafe (Konventionalstrafe) liegt vor wenn der Schuldner dem Gläubiger für den Fall daß er seine Verbindlichkeit nicht oder nicht in gehöriger Weise erfüllt die Zahlung einer Geldsumme als Strafe verspricht"
        },
        {
            "@language": "de",
            "@value": "wird zwischen zwei Vertragsparteien für den Fall vereinbart daß eine Partei die geschuldete Leistung nicht wie vereinbart erbringt"
        },
        {
            "@language": "en",
            "@value": "a fixed sum agreed upon at the outset of a contractual arrangementas the proper compensation to be paid to the injured party in the event of breach"
        },
        {
            "@language": "de",
            "@value": "eine vereinbarte Festsumme zu Anfang der vertraglichen Abmachungen als angemessener Ausgleich der im Falle eines (Vertrags-)Bruches an die geschaedigte Partei zu zahlen ist"
        }
    ]
