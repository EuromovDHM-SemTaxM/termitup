    "skos:definition": [
        {
            "@language": "de",
            "@value": "vertragliche Überlassung des Gebrauchs und der Fruchtziehung einer Sache oder eines Rechts gegen Entgelt den. sog. Pachtzins"
        },
        {
            "@language": "en",
            "@value": "tenants right of possession for a specific period of time under a lease agreement"
        },
        {
            "@language": "de",
            "@value": "Vertrag der den Verpaechter verpflichtet dem Paechter gegen Zahlung des Pachtzinses den Gebrauch des gepachteten Gegenstandes und darueber hinaus den Genuss der bei ordnungsgemaesser Wirtschaft anfallenden Fruechte waehrend der Pachtzeit zu gewaehren"
        }
