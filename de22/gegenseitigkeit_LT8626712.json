    "skos:definition": [
        {
            "@language": "en",
            "@value": "recognition by a court of a judgment given by a foreign court pursuant to a treaty providing that the courts of each country shall recognise judgments given by the courts of the other"
        },
        {
            "@language": "es",
            "@value": "Concesión de derechos a innovaciones o creaciones originadas en algún país en la proporción en que se les concedan a las que se originen en el propio."
        },
        {
            "@language": "en",
            "@value": "principle whereby one country (A) makes the granting of legal protection for intellectual property products from another country (B) conditional upon country B's granting legal protection for intellectual property products from country A"
        }
    ]
