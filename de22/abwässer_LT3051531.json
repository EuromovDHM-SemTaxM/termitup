    "skos:definition": [
        {
            "@language": "en",
            "@value": "water from any combination of domestic industrial commercial or agricultural activities surface runoff [ IATE:1415844 ]/stormwater [ IATE:1427108 ] and any sewer inflow/infiltration"
        },
        {
            "@language": "de",
            "@value": "Wasser das aus überbauten Gebieten abgeleitet werden muß darunter Abwasser aus Haushalt Gewerbe und Industrie einschließlich Kühlwasser sowie Regenwasser Schneeschmelz-und Sickerwasser ob verschmutzt oder unverschmutzt ;Nicht trinkbare gesundheitsgefährdende verschmutzte Haushalts-und Industrieabwässer"
        },
        {
            "@language": "es",
            "@value": "Aguas de escorrentía [ IATE:1118855 ] pluvial que suelen estar mezcladas con estiércol [ IATE:1255100 ] agua procedente de la limpieza de superficies (p. ej. suelos) y equipos y agua generada por el funcionamiento de sistemas de depuración del aire."
        },
        {
            "@language": "en",
            "@value": "rainwater run-off [ IATE:1118855 ] commonly mixed with manure [ IATE:1255100 ] water derived from the cleaning of surfaces (e.g. floors) and equipment and water derived from the operation of air cleaning systems on a farm"
        },
        {
            "@language": "nl",
            "@value": "afvloeiend hemelwater gewoonlijk gemengd met mest water afkomstig van het reinigen van oppervlakken (bv. vloeren) en uitrusting alsmede water afkomstig van luchtzuiveringssystemen"
        },
        {
            "@language": "es",
            "@value": "flujo de agua tratada o no procedente de residuos domésticos urbanos agrícolas o industriales"
        },
        {
            "@language": "en",
            "@value": "some sewage water or other liquid partially or completely treated or in its natural state flowing out of a reservoir basin or treatment plant"
        },
        {
            "@language": "de",
            "@value": "Das in Haushalt Gewerbe Landwirtschaft oder Industrie anfallende gebrauchte Wasser behandelt oder unbehandelt"
        },
        {
            "@language": "nl",
            "@value": "Water dat in huishouding of industrie zijn diensten gedaan heeft en gewoonlijk met verschillende stoffen beladen is"
        },
        {
            "@language": "es",
            "@value": "agua utilizada por el hombre para su consumo su actividad o su confort"
        },
        {
            "@language": "en",
            "@value": "waste matter transported through sewers"
        },
        {
            "@language": "es",
            "@value": "agua que contiene en su seno organismos patógenos o sustancias minerales u orgánicas de diverso origen que la hace inadecuada para la mayoría de los usos"
        },
        {
            "@language": "en",
            "@value": "a water which is charged with or characterised by offensive or noinsome matter; water containing sewage or other contaminants"
        }
    ]
