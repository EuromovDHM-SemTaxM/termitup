    "skos:definition": [
        {
            "@language": "en",
            "@value": "process designed to eradicate all viable forms of microbial life including viruses bacterial spores and mycobacteria from the surface of an article or in a fluid to prevent disease transmission associated with use of that item"
        },
        {
            "@language": "de",
            "@value": "Zerstörung aller lebenden Mikroorganismen zur Ausschaltung von Infektionsquellen ;Beseitigung aller kontaminierenden Mikroorganismen mittels physikalischer Verfahren"
        },
        {
            "@language": "de",
            "@value": "Unfruchtbarmachung"
        },
        {
            "@language": "nl",
            "@value": "het onvruchtbaar maken waarbij aan een levend organisme de mogelijkheid wordt ontnomen zich voort te planten"
        },
        {
            "@language": "es",
            "@value": "Proceso destinado a eliminar toda forma de vida microbiana de un dispositivo que ya ha sido previamente esterilizado."
        },
        {
            "@language": "en",
            "@value": "process of resterilising single-use or multiple-use medical devices as part of their reprocessing"
        },
        {
            "@language": "en",
            "@value": "an act or process of destroying all forms of microbial life on and in an object."
        }
