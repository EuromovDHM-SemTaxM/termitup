    "skos:definition": [
        {
            "@language": "es",
            "@value": "Delito consistente en encerrar o detener a otro ilegalmente privándole de su libertad."
        },
        {
            "@language": "en",
            "@value": "the illegal confinement of one individual against his or her will by another individual in such a manner as to violate the confined individual's right to be free from restraint of movement"
        },
        {
            "@language": "de",
            "@value": "widerrechtliches Einsperren/ Gefangennehmen e. Person"
        },
        {
            "@language": "nl",
            "@value": "het iemand anders wederrechtelijk van de vrijheid beroven of beroofd houden"
        },
        {
            "@language": "de",
            "@value": "eine Straftat die jemand begeht indem er einen anderen widerrechtlich gefangen hält oder ihm auf andere Weise die persönliche Freiheit entzieht."
        }
