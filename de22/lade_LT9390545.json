    "skos:definition": [
        {
            "@language": "en",
            "@value": "quantity of any itempackaged or unpackagedwhich is arranged on a pallet in a specified manner and securely strapped or fastened thereto so that the whole is handled as a unit"
        },
        {
            "@language": "en",
            "@value": "A swinging frame that carries the cylinder of a jacquard machine."
        },
        {
            "@language": "en",
