    "skos:definition": [
        {
            "@language": "es",
            "@value": "«La sociedad [civil] es un contrato por el cual dos o más personas se obligan a poner en común dinero bienes o industria con ánimo de partir entre sí las ganancias» (art. 1665 del Código Civil)."
        },
        {
            "@language": "en",
            "@value": "a corporation having no issued shares"
