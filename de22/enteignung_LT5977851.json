    "skos:definition": [
        {
            "@language": "es",
            "@value": "cualquier forma de privación singular de la propiedad privada o de derechos e intereses patrimoniales legítimos cualesquiera que fueran las personas o entidades a que pertenezcan acordada imperativamente ya implique venta permuta censo arrendamiento ocupación temporal o nueva cesación de su ejercicio"
        },
        {
            "@language": "en",
            "@value": "the taking by the state of private property for public purposes"
        },
        {
            "@language": "de",
            "@value": "die zwangweise (gerichtlich verfügte) Ablösung des Rechts an einem Grundstück durch Gelder oder Ersatzgrundstücke"
        },
        {
            "@language": "en",
            "@value": "1.To deprive an owner of property especially by taking it for public use."
        },
        {
            "@language": "de",
            "@value": "in DE gemäss Art.14 III 1 GG nur zum Wohle der Allgemeinheit zulässig und an eine Entschädigung (nicht notwendigerweise zum vollen Wert) geknüpft"
        }
