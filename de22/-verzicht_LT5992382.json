    "skos:definition": [
        {
            "@language": "es",
            "@value": "Desistimiento o dejación voluntaria que una persona hace de un derecho posesión posición  título etc. que le corresponden."
        },
        {
            "@language": "en",
            "@value": "action of giving up or surrendering or tacitly abandoning a right position possession title etc."
        },
        {
            "@language": "nl",
            "@value": "eenzijdige akte waarbij een partij hetzij uitdrukkelijk hetzij stilzwijgend afziet van een recht dat mag worden prijsgegeven"
        },
        {
            "@language": "de",
            "@value": "die Aufhebung einer Verbindlichkeit durch Rechtsaufgabe des Gläubigers. Der Verzicht kann entgeltlich oder unentgeltlich sein bedarf aber der Zustimmung des Gläubigers."
        },
        {
            "@language": "en",
            "@value": "the leaving to an employer by an employee of a part of his salary so that the employer can use the money forgone to buy pension or life assurance benefits"
        }
