    "skos:definition": [
        {
            "@language": "en",
            "@value": "the sloping side of any hollow in the ground especially when bordering a river."
        },
        {
            "@language": "en",
            "@value": "the portion of the river bank that is located above the low water level and below the foreshore level."
        },
        {
            "@language": "de",
            "@value": "der Teil des Flussufers der oberhalb des Niedrigwasserspiegels und unterhalb der Voruferhoehe liegt."
        },
        {
            "@language": "en",
            "@value": "the portion of bank below low water level often extending quite a distance horizontally to the river bed."
        },
        {
            "@language": "de",
            "@value": "der Teil des Ufers unter dem Niedrigwasserspiegel der sich oft ueber eine groessere Strecke waagerecht zum Flussbett hin erstreckt."
        }
