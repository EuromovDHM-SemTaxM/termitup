    "skos:definition": [
        {
            "@language": "en",
            "@value": "part of the principle enshrined in Article 157 of the Treaty on the Functioning of the European Union ensuring that men and women are treated equally in their working life including with regard to their pay"
        },
        {
            "@language": "de",
