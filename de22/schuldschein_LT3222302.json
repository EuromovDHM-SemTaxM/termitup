    "skos:definition": [
        {
            "@language": "en",
            "@value": "unconditional promise in writing made by one person to another and signed by the maker engaging to pay a specified sum of money to (or to the order of) a specified person or to the bearer either on demand or at a fixed or determinable future time"
        },
        {
            "@language": "de",
