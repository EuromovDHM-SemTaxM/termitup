    "skos:definition": [
        {
            "@language": "es",
            "@value": "fase de aprendizaje negativo durante la cual el sistema muestrea aleatoriamente los estados según su distribución de Boltzmann y el aprendizaje de Hebbs se produce con un coeficiente negativo"
        },
        {
            "@language": "en",
            "@value": "negative learning phase during which the system randomly samples states according to their Boltzmann distribution and Hebbian learning occurs with a negative coefficient"
        },
        {
            "@language": "de",
            "@value": "da nicht alle lokalen Minima tatsächlich gelerntes Wissen repräsentieren versucht man diese mittels umgekehrtenen Lernens zu eliminieren. Dabei wird die normale Lernregel jedoch mit umgekehrtem Vorzeichen auf Zustände des Systems angewendet die dieses von zufälligen Anfangsbedingungen aus erreicht"
        },
        {
            "@language": "nl",
            "@value": "negatieve leerfase waarbij het systeem willekeurig toestanden verzamelt volgens de Boltzmann verdeling en het Hebbiaans leren plaatsvindt met een negatieve coëfficiënt"
        }
    ]
