    "skos:definition": [
        {
            "@language": "es",
            "@value": "El incumplimiento de una obligación o de una cláusula de un contrato y en particular la obligación de pago en el plazo establecido."
        },
        {
            "@language": "en",
            "@value": "failure to fulfil an obligation especially to repay a loan or appear in a law court"
        },
        {
            "@language": "de",
            "@value": "Nichterfüllen einer Verpflichtung (bis) zum gesetzten Zeitpunkt"
        },
        {
