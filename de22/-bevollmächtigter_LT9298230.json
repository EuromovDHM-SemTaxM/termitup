    "skos:definition": [
        {
            "@language": "en",
            "@value": "a person authorised by a citizen to deal with the authorities on her or his behalf"
        },
        {
            "@language": "de",
            "@value": "zum Handeln im Namen einer anderen Person Ermächtigter"
        },
        {
            "@language": "en",
            "@value": "person who is authorised to act for another"
        },
        {
            "@language": "de",
            "@value": "öffentlich oder privat Beauftragter"
        },
        {
            "@language": "nl",
            "@value": "persoon die in naam en voor rekening van een ander een of meer rechtshandelingen mag verrichten"
        },
        {
            "@language": "es",
            "@value": "En el mandato persona que acepta el cargo del mandante."
        },
        {
            "@language": "en",
            "@value": "legal owner of property who holds it on behalf of the beneficial owner"
        },
        {
            "@language": "de",
            "@value": "per Willenserklärung bzw. durch deren Empfang für eine andere Person in deren Namen handelnder und entscheidender Stellvertreter"
        },
        {
            "@language": "nl",
            "@value": "persoon of (financiële) instelling die 'in naam' eigenaar is van aandelen of een bedrijf en voor de werkelijke aandeelhouder een aantal diensten verricht"
        },
        {
            "@language": "nl",
            "@value": "\"een in de Gemeenschap gevestigde natuurlijke of rechtspersoon die schriftelijk door een fabrikant is gemachtigd om namens hem specifieke taken te vervullen\""
        }
