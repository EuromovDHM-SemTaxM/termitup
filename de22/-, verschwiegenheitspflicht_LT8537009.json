    "skos:definition": [
        {
            "@language": "en",
            "@value": "obligation imposed on data protection commissioners to maintain silence about the facts with which they may become acquainted during their official activities"
        },
        {
            "@language": "de",
            "@value": "Pflicht der Datenschutzbeauftragten über die ihnen bei ihrer Tätigkeit bekanntgewordenen Angelegenheiten Verschwiegenheit zu bewahren"
        },
        {
            "@language": "es",
            "@value": "respeto de la confidencialidad de las historias clínicas y de las comunicaciones entre los proovedores de asistencia sanitaria y sus pacientes"
        },
        {
            "@language": "en",
            "@value": "practice of maintaining the security and confidentiality of patient records and of communications between patients and health care professionals"
        },
        {
            "@language": "de",
            "@value": "rechtliche Verpflichtung von Ärzten ihnen anvertraute Informationen und Daten nicht unbefugt an Dritte weiterzugeben."
        },
        {
            "@language": "nl",
            "@value": "het vertrouwelijk houden van communicatie van aanbieders van gezondheidszorg waaronder therapeuten met hun patiënten"
        },
        {
            "@language": "en",
            "@value": "the right to withhold evidence in legal proceedings because of the existence of medical confidentiality"
        },
        {
            "@language": "de",
            "@value": "rechtliche Verpflichtung von Angehörigen medizinischer Berufe über das was ihnen in ihrer Eigenschaft als Arzt anvertraut oder bekannt geworden ist auch nach dem Tod des Patienten zu schweigen"
        },
        {
            "@language": "nl",
            "@value": "het verbod dat hulpverleners is opgelegd om over patiënten iets tegen anderen te zeggen."
        }
    ]
