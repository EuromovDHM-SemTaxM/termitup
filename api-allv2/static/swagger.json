    }
  ],
  "paths": {
    "/term": {
      "get": {
        "tags": [
          "Search Terms"
        ],
        "description": "Multiple output of result",
        "summary": "---Get--",
        "parameters": [
          {
            "in": "query",
            "name": "term",
            "required": true,
            "description": "Term to be enriched",
            "schema": {
              "$ref": ""
            }
          },
          {
            "in": "query",
            "name": "source_language",
            "required": true,
            "description": "Language of the source term to be enriched, separated by blank spaces (en es de nl...)",
            "schema": {
              "$ref": ""
            }
          },
          {
            "in": "query",
            "name": "target_languages",
            "required": true,
            "description": "Languages in which the retrieved information is desired",
            "schema": {
              "$ref": ""
            }
          },
          {
            "in": "query",
            "name": "context",
            "required": false,
            "description": "Context in which the source term appears (Optional)",
            "schema": {
              "$ref": ""
            }
          },
          {
            "in": "query",
            "name": "wsid",
            "required": true,
            "description": "Write yes if you want to call the disambiguation service and no if you don't.",
            "schema": {
              "$ref": ""
            }
          },
          {
            "in": "query",
            "name": "relation_validation",
            "required": true,
            "description": "Write si yes you want to call the relation validation service and no if you don't.",
            "schema": {
              "$ref": ""
            }
          },
          {
            "in": "query",
            "name": "schema",
            "required": true,
            "description": "Schema of your set of terms (labour law, bio, administration...).",
            "schema": {
              "$ref": ""
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Term"
                }
              }
            }
          },
          "404": {
            "description": "Failed. No more requests.",
            "content": {}
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Term": {
        "type": "object",
        "properties": {
          "context": {
            "type": "string"
          },
          "id": {
