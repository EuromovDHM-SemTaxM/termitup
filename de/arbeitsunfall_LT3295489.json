    "skos:definition": [
        {
            "@language": "es",
            "@value": "Toda lesión corporal que el trabajador sufra con ocasión o por consecuencia del trabajo que ejecute por cuenta ajena."
        },
        {
            "@language": "en",
            "@value": "discrete occurrence during the course of work which leads to physical or mental harm"
        },
        {
            "@language": "de",
            "@value": "ein während der Arbeit eintretendes deutlich abzugrenzendes Ereignis das zu einem physischen oder psychischen Schaden führt"
        },
        {
            "@language": "nl",
            "@value": "ongeval dat zich voordoet tijdens de uitvoering van de arbeidsovereenkomst"
        },
        {
            "@language": "en",
            "@value": "1.Accident occurring in the course of the employment and caused by inherent or related factors arising from the operation of materials of one's occupation."
