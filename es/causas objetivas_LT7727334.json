    "skos:definition": [
        {
            "@language": "es",
            "@value": "Trabajador cuyo contrato es extinguido a voluntad de su empleador por causas objetivas (económicas técnicas organizativas o productivas)."
        },
        {
            "@language": "en",
            "@value": "worker whose employment is ended prematurely by redundancy or whose contract is not renewed due to economic reasons"
        },
        {
            "@language": "de",
            "@value": "Arbeitnehmer dessen Arbeitsverhältnis aus wirtschaftlichen Gründen vorzeitig durch Entlassung endet oder dessen Vertrag aus wirtschaftlichen Gründen nicht erneuert wird"
        },
        {
            "@language": "nl",
            "@value": "werknemer wiens arbeidsovereenkomst voortijdig is beëindigd door gedwongen ontslag of wiens arbeidsovereenkomst om economische redenen niet is verlengd"
        }
